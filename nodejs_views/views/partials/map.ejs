<h3>Map</h3>
<div id="map"></div>
<script>
    function initMap() {
        var Hong_Kong = {
            lat: 22.28552,
            lng: 114.15769
        };
        var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 4,
            center: Hong_Kong
        });
        var marker = new google.maps.Marker({
            position: Hong_Kong,
            map: map
        });
    }
</script>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAym-222qini_lHt9Qc5PEajGhCN3BqudY&callback=initMap">
</script>